<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="dubbo-test-ref"/>

    <!-- 使用multicast广播注册中心暴露服务地址 -->
    <!--  <dubbo:registry address="multicast://224.5.6.7:1234" /> -->
    <dubbo:registry id="mts-test-registry" protocol="zookeeper"
                    address="10.32.64.12:2181,10.32.64.19:2181,10.32.64.161:2181 " check="false"
                    group="mts-wx-center-test"/>

    <!-- 声明需要暴露的服务接口 -->
    <dubbo:reference id="testService" interface="com.benmu.mts.wx.center.controller.TestService"
                     version="1.0"/>

</beans>